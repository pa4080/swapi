{
  "cSpell.words": [
    "Agung",
    "Argel",
    "Arrayable",
    "autoimport",
    "Bootcamp",
    "Codeholic",
    "Ekene",
    "esversion",
    "headlessui",
    "Heroicons",
    "intelephense",
    "LAAG",
    "Laravel",
    "Listbox",
    "Naseel",
    "phpdoc",
    "predeploy",
    "signup",
    "Signup",
    "tailwindcss",
    "Waltograph"
  ],
  "markdownlint.config": {
    "MD033": false
  },
  "php.problems.exclude": {
    "/": [406],
    "vendor/": true
  },
  "[javascript]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[typescript]": {
    "editor.defaultFormatter": "vscode.typescript-language-features"
  },
  "[yaml]": {
    "editor.formatOnSave": false
  },
  "editor.formatOnSave": true,
  "editor.defaultFormatter": null,
  "spellright.language": ["en-US-10-1.", "en-US-9-0."],
  "spellright.documentTypes": ["markdown", "latex", "plaintext", "php"],
  "intelephense.diagnostics.undefinedMethods": false,
  "intelephense.phpdoc.useFullyQualifiedNames": true,
  "php.completion.autoimport-docblock": "fqn",
  "php.suggest.basic": true,
  "php.completion.parameters": "parameters",
  "npm-intellisense.importQuotes": "\"",
  "javascript.preferences.quoteStyle": "double",
  "typescript.preferences.quoteStyle": "double",
  "prettier.semi": true,
  "prettier.singleQuote": false,
  "prettier.jsxSingleQuote": false,
  "prettier.trailingComma": "none",
  "prettier.singleAttributePerLine": true,
  "prettier.bracketSpacing": true,
  "prettier.printWidth": 90,
  "eslint.format.enable": true,
  "eslint.run": "onType",
  "javascript.format.insertSpaceAfterOpeningAndBeforeClosingNonemptyBraces": false,
  "css.lint.unknownAtRules": "ignore",
  "reactSnippets.settings.prettierEnabled": true,
  "reactSnippets.settings.importReactOnTop": true
}
